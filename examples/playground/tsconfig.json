{
    "extends": "./tsconfig.node.json",
    "compilerOptions": {
        "jsx": "react-jsx",
        "strict": true,
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true
    },
    "include": [
        "src"
    ]
}